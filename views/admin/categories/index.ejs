<%- include('../../partials/header.ejs')  %>
<%- include('../../partials/navbar.ejs')  %>

<div class="container">
    <hr>
    <h2>Categories</h2>
    <hr>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>ID</th>
                <th>Title</th>
                <th>Slug</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            
            <% categories.forEach(category => { %>
                <tr>
                    <th><%=category.id%></th>
                    <th><%=category.title%></th>
                    <th><%=category.slug%></th>
                    <th></th>
                </tr>

            <% }) %>
        </tbody>
    </table>


<%- include('../../partials/footer.ejs')  %>